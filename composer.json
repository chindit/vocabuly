{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "bentools/doctrine-native-enums": "^0.1.0",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "sensio/framework-extra-bundle": "^v6.2.4",
        "symfony/console": "6.0.1",
        "symfony/dotenv": "6.0.1",
        "symfony/flex": "^2",
        "symfony/form": "6.0.1",
        "symfony/framework-bundle": "6.0.1",
        "symfony/proxy-manager-bridge": "6.0.1",
        "symfony/runtime": "6.0.0",
        "symfony/security-bundle": "6.0.1",
        "symfony/templating": "6.0.1",
        "symfony/twig-bundle": "6.0.1",
        "symfony/validator": "6.0.1",
        "symfony/yaml": "6.0.1",
        "twig/extra-bundle": "^3.3.4",
        "twig/twig": "^3.3.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-doctrine": "^1.0",
        "symfony/debug-bundle": "6.0.0",
        "symfony/maker-bundle": "^1.36",
        "symfony/monolog-bundle": "^3.7",
        "symfony/stopwatch": "6.0.0",
        "symfony/web-profiler-bundle": "6.0.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
